{
  "name": "starhs-api-proxy-aws-lambda",
  "description": "A proxy for the staRHs API running on AWS lambda",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=testing mocha --compilers js:babel-register --reporter=dot --timeout=1350 test/\\*\\*/\\*.js",
    "test:coverage": "NODE_ENV=testing istanbul cover _mocha -- --compilers js:babel-core/register --reporter=dot --timeout=1350 test/\\*\\*/\\*.js",
    "test:coverage-travis": "NODE_ENV=testing istanbul cover _mocha --report lcovonly -- --compilers js:babel-register --reporter=dot --timeout=1350 test/\\*\\*/\\*.js && cat ./coverage/lcov.info | codacy-coverage",
    "lint": "standard && flow check",
    "build": "babel ./ -d ./build --ignore \"node_modules/,build/,coverage/,test/\"",
    "precommit": "npm run lint && npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ResourcefulHumans/starhs-api-proxy-aws-lambda.git"
  },
  "keywords": [
    "simple",
    "social",
    "realtime"
  ],
  "author": "Resourceful Humans <info@resourceful-humans.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ResourcefulHumans/starhs-api-proxy-aws-lambda/issues"
  },
  "homepage": "https://github.com/ResourcefulHumans/starhs-api-proxy-aws-lambda#readme",
  "dependencies": {
    "aws-sdk": "^2.6.7",
    "bluebird": "^3.4.6",
    "glob": "^7.1.0",
    "joi": "^9.1.0",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "nconf": "^0.8.4",
    "request": "^2.75.0",
    "request-promise": "^4.1.1",
    "rheactor-models": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "flow-bin": "^0.35.0",
    "husky": "^0.11.9",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "mockery": "^2.0.0",
    "sinon": "^1.17.6",
    "standard": "^8.3.0",
    "validate-commit-msg": "^2.8.2",
    "semantic-release": "^4.3.5"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  }
}
